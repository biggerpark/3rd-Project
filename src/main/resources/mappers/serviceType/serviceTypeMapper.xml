<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.green.jobdone.serviceType.ServiceTypeMapper">

    <insert id="insRoom" useGeneratedKeys="true" keyProperty="roomId">
        INSERT INTO room
        SET
        serviceId = #{serviceId}

    </insert>
    <insert id="insChat" useGeneratedKeys="true" keyProperty="chatId">
        INSERT INTO room
        SET
        serviceId = #{serviceId}

    </insert>

</mapper>